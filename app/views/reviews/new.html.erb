<h1>Page unused because of the embedded form in restaurants#show</h1>
<div class="row">
  <div class="col">
    <h1>Share a review</h1>
    <%= simple_form_for [ @restaurant, @review ] do |f| %>
      <%= f.input :rating %>
      <%= f.input :content %>
      <%= f.submit "Submit review", class: "btn btn-primary" %><%#= link_to "Cancel", restaurant_path(@restaurant), class: 'btn btn-primary ml-3' %>
    <% end %>
  </div>
</div>
